@model IEnumerable<ITServiceProvider.Models.Client>

@{
                /**/

                ViewBag.Title = "HappyClient";
}



<div class="container">
    <div class="row">
        <h2>Happy Client</h2>

        @foreach (var item in Model)
        {
            <div class="col-md-4">
                <img src="~/user.png" class="img-thumbnail" />
                <h2>@Html.DisplayFor(modelItem => item.Name) </h2>
                <hr />
                <p>  @Html.DisplayNameFor(model => model.services) </p>
            </div>
        }




    </div>
</div>


